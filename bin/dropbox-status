#!/bin/bash
dropboxes=".dropbox-pfix .dropbox-jimdo"
for dropbox in $dropboxes
do
    HOME="/home/$USER"
    HOME="$HOME/$dropbox"
    export HOME
    echo -n "$dropbox: "
    #DROPBOX_BIN=${HOME}/.dropbox-dist/$(ls ${HOME}/.dropbox-dist | grep dropbox-lnx)/dropbox
    python2 $(which dropbox.py) status
done
